
<%- include('../layouts/header.ejs')%>

<%- include('../layouts/sidebar.ejs')%>
   
<main class="main-content position-relative max-height-vh-100 h-100 border-radius-lg ">



   <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>


  
    <form style="padding: 80px;" method="post" enctype="multipart/form-data">
        <div class="form-group">
          <label>Name</label>
          <input type="text" name="name" class="form-control" placeholder="Enter the Name of Banner">
        </div>
        <br><br>
        <div class="form-group">
            <label >Image</label>
            <input type="file"  name="image" multiple required>
          </div>
        <br><br>
        <div class="form-group">
          <label>About</label>
          <textarea placeholder="Type of Banner" class="form-control" name="description" rows="3"></textarea>
        </div>
        <br>
        <button type="submit" class="btn btn-success">Add</button>
        <% if(typeof message !=="undefined") { %>
        <h5 style="font-size: 14px;color: brown;"><%=message %></h5>
        <% } %>
      </form>
      